package com.sist.dao;
import org.jsoup.nodes.Element;

public class tutor_VO {
	private int no;
	private int cno;
	private String coach_name;
	private String coach_detail;
	private String poster;
	private String title;
	private String content;
	private String price;
	private String city;
	
	private String coach_image;
	
	
	public int getNo() {
		return no;
	}
	public void setNo(int no) {
		this.no = no;
	}
	public int getCno() {
		return cno;
	}
	public void setCno(int cno) {
		this.cno = cno;
	}
	public String getCoach_image() {
		return coach_image;
	}
	public void setCoach_image(String coach_image) {
		this.coach_image = coach_image;
	}
	private String review_star; // 전체 리뷰 별점의 평균을 내야할 듯?
	private int review_counts; // 리뷰 데이터 축적되면 그 갯수 카운팅
	
	private String indate; // 협의 후 결정이 많아서 데이터 긁어와도 소용이 없을 듯!!
	
	
	
	public String getIndate() {
		return indate;
	}
	public void setIndate(String indate) {
		this.indate = indate;
	}
	public String getCoach_name() {
		return coach_name;
	}
	public void setCoach_name(String coach_name) {
		this.coach_name = coach_name;
	}
	public String getCoach_detail() {
		return coach_detail;
	}
	public void setCoach_detail(String coach_detail) {
		this.coach_detail = coach_detail;
	}
	public String getPoster() {
		return poster;
	}
	public void setPoster(String poster) {
		this.poster = poster;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getReview_star() {
		return review_star;
	}
	public void setReview_star(String review_star) {
		this.review_star = review_star;
	}
	public int getReview_counts() {
		return review_counts;
	}
	public void setReview_counts(int review_counts) {
		this.review_counts = review_counts;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	
	
}
